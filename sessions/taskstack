22
normal! 0
wincmd w
exe '1resize ' . ((&lines * 20 + 35) / 70)
exe '2resize ' . ((&lines * 24 + 35) / 70)
exe '3resize ' . ((&lines * 22 + 35) / 70)
exe 'vert 3resize ' . ((&columns * 57 + 57) / 115)
exe '4resize ' . ((&lines * 22 + 35) / 70)
exe 'vert 4resize ' . ((&columns * 57 + 57) / 115)
tabnext 1
if exists('s:wipebuf')
  silent exe 'bwipe ' . s:wipebuf
endif
