wincmd w
exe 'vert 1resize ' . ((&columns * 77 + 117) / 234)
exe 'vert 2resize ' . ((&columns * 78 + 117) / 234)
exe 'vert 3resize ' . ((&columns * 77 + 117) / 234)
tabedit ~/sandbox/personal/writing/story.txt
set splitbelow splitright
wincmd _ | wincmd |
vsplit
1wincmd h
wincmd w
wincmd t
set winheight=1 winwidth=1
