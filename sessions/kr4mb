set cpo&vim
nmap <buffer> <NL> :call TaskstackMoveItemDown()
nmap <buffer>  :call TaskstackMoveItemUp()
nmap <buffer> $ :call TaskstackEOL()
nmap <buffer> :w :call TaskstackAutosaveReminder()
nmap <buffer> LL :call TaskstackScratch()
nmap <buffer> NN :call TaskstackNewItem()
nmap <buffer> QQ :call TagstackCompleteItem(g:completed_prefix)
nmap <buffer> XX :call TagstackCompleteItem(g:aborted_prefix)
nmap <buffer> ZZ :call TaskstackHide()
nmap <buffer> <F8> :call TaskstackGroups()
imap <buffer> NN :call TaskstackNewItem()
imap <buffer> QQ :call TagstackCompleteItem(g:completed_prefix)
imap <buffer> XX :call TagstackCompleteItem(g:aborted_prefix)
imap <buffer> ZZ :call TaskstackHide()
